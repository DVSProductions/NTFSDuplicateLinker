<Window x:Class="NTFSDuplicateLinker.Filters"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NTFSDuplicateLinker"
        mc:Ignorable="d"
        Title="Filters" Height="200" Width="150">
    <Grid>
		<GroupBox Header="Filters">
			<StackPanel x:Name="spFilter">
			</StackPanel>
		</GroupBox>
		<Button Content="Apply" VerticalAlignment="Bottom"/>
	</Grid>
</Window>
